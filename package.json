{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "forever start dist/src/index.js",
    "stop": "forever stopall",
    "restart": "forever restart dist/src/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "node test.js",
    "deploy": "yarn && yarn build && yarn start",
    "crawl": "node dist/src/crawler.js"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.23",
    "@types/puppeteer": "^3.0.1",
    "express": "^4.15.2",
    "forever": "^3.0.2",
    "puppeteer": "^5.1.0",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "got": "^11.3.0",
    "tape": "^4.7.0",
    "ts-node": "^8.10.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
